<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <!--#region
    attribute_types.xsd -->
  <!-- general attributes -->
  <xs:simpleType name="twoIntsType">
    <xs:restriction base="xs:string">
      <xs:pattern value="\s*[0-9]+\s+[0-9]+\s*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="upToTwoIntsType">
    <xs:restriction base="xs:string">
      <xs:pattern value="\s*([0-9]+)(\s+[0-9]+)?\s*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="threeIntsType">
    <xs:restriction base="xs:string">
      <xs:pattern value="\s*[0-9]+(\s+[0-9]+){2}\s*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="upToThreeIntsType">
    <xs:restriction base="xs:string">
      <xs:pattern value="\s*([0-9]+)(\s+[0-9]+){0,2}\s*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="infIntsType">
    <xs:restriction base="xs:string">
      <xs:pattern value="\s*([0-9]+(\s+[0-9]+)*\s*)?" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="twoRealsType">
    <xs:restriction base="xs:string">
      <xs:pattern
        value="\s*[+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?\s+[+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?\s*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="upToTwoRealsType">
    <xs:restriction base="xs:string">
      <xs:pattern
        value="\s*([+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?)(\s+([+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?))?\s*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="threeRealsType">
    <xs:restriction base="xs:string">
      <xs:pattern
        value="\s*[+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?(\s+[+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?){2}\s*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="upToThreeRealsType">
    <xs:restriction base="xs:string">
      <xs:pattern
        value="\s*([+-]?([0-9]+(\.[0-9]*)?|\.[0-9]+)([eE][+-]?[0-9]+)?)(\s+([+-]?([0-9]+(\.[0-9]*)?|\.[0-9]+)([eE][+-]?[0-9]+)?)){0,2}\s*" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="fourRealsType">
    <xs:restriction base="xs:string">
      <xs:pattern
        value="\s*[+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?(\s+[+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?){3}\s*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="upToFourRealsType">
    <xs:restriction base="xs:string">
      <xs:pattern
        value="\s*([+-]?([0-9]+(\.[0-9]*)?|\.[0-9]+)([eE][+-]?[0-9]+)?)(\s+([+-]?([0-9]+(\.[0-9]*)?|\.[0-9]+)([eE][+-]?[0-9]+)?)){0,3}\s*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="fiveRealsType">
    <xs:restriction base="xs:string">
      <xs:pattern
        value="\s*[+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?(\s+[+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?){4}\s*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="upToFiveRealsType">
    <xs:restriction base="xs:string">
      <xs:pattern
        value="\s*([+-]?([0-9]+(\.[0-9]*)?|\.[0-9]+)([eE][+-]?[0-9]+)?)(\s+([+-]?([0-9]+(\.[0-9]*)?|\.[0-9]+)([eE][+-]?[0-9]+)?)){0,4}\s*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="sixRealsType">
    <xs:restriction base="xs:string">
      <xs:pattern
        value="\s*[+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?(\s+[+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?){5}\s*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="upToSixRealsType">
    <xs:restriction base="xs:string">
      <xs:pattern
        value="\s*([+-]?([0-9]+(\.[0-9]*)?|\.[0-9]+)([eE][+-]?[0-9]+)?)(\s+([+-]?([0-9]+(\.[0-9]*)?|\.[0-9]+)([eE][+-]?[0-9]+)?)){0,5}\s*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="sevenRealsType">
    <xs:restriction base="xs:string">
      <xs:pattern
        value="\s*[+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?(\s+[+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?){6}\s*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="upToSevenRealsType">
    <xs:restriction base="xs:string">
      <xs:pattern
        value="\s*([+-]?([0-9]+(\.[0-9]*)?|\.[0-9]+)([eE][+-]?[0-9]+)?)(\s+([+-]?([0-9]+(\.[0-9]*)?|\.[0-9]+)([eE][+-]?[0-9]+)?)){0,6}\s*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tenRealsType">
    <xs:restriction base="xs:string">
      <xs:pattern
        value="\s*[+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?(\s+[+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?){9}\s*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="upToTenRealsType">
    <xs:restriction base="xs:string">
      <xs:pattern
        value="\s*([+-]?([0-9]+(\.[0-9]*)?|\.[0-9]+)([eE][+-]?[0-9]+)?)(\s+([+-]?([0-9]+(\.[0-9]*)?|\.[0-9]+)([eE][+-]?[0-9]+)?)){0,9}\s*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="infRealsType">
    <xs:restriction base="xs:string">
      <xs:pattern
        value="\s*([+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?(\s+[+-]?(\d+(\.\d*)?|\.\d+)([eE][+-]?\d+)?)*\s*)?" />
    </xs:restriction>
  </xs:simpleType>


  <xs:simpleType name="solimpType">
    <xs:restriction base="xs:string">
      <xs:pattern
        value="(([+-]?(([0-9]+.{0,1}[0-9]*)|([0-9]*.{0,1}[0-9]+))(e-?[0-9]+)? ){2}[+-]?(([0-9]+.{0,1}[0-9]*)|([0-9]*.{0,1}[0-9]+))(e-?[0-9]+)?)|(([+-]?(([0-9]+.{0,1}[0-9]*)|([0-9]*.{0,1}[0-9]+))(e-?[0-9]+)? ){4}[+-]?(([0-9]+.{0,1}[0-9]*)|([0-9]*.{0,1}[0-9]+))(e-?[0-9]+)?)" />
